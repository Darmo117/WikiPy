        "title": "Page rename log",
        "entry": "${author:user} has renamed the page ${full_page_title:link} to ${new_full_page_title:link} without creating a redirection. (${reason})",
        "entry_redirect": "${author:user} has renamed the page ${full_page_title:link} to ${new_full_page_title:link} and created a redirection. (${reason})"
      }
