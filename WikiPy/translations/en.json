        "reason": "Account creation."
      },
      "user_rename": {
        "title": "User rename log",
        "entry": {
          "feminine": "${author:user} has renamed user ${target_user:link} to ${subject_new_username:user}. (${reason})",
          "masculine": "${author:user} has renamed user ${target_user:link} to ${subject_new_username:user}. (${reason})",
          "neutral": "${author:user} has renamed user ${target_user:link} to ${subject_new_username:user}. (${reason})"
        }
      },
      "user_block": {
        "title": "User block log",
        "entry": {
          "feminine": "${author:user} has blocked user ${target_user:link} until ${expiration_date:date}. (${reason})",
          "masculine": "${author:user} has blocked user ${target_user:link} until ${expiration_date:date}. (${reason})",
          "neutral": "${author:user} has blocked user ${target_user:link} until ${expiration_date:date}. (${reason})"
        }
      },
      "user_group_change": {
        "title": "User group change log",
        "entry": {
          "added": "${target_user:user} was added to the group “${group:group}” by ${author:user}. (${reason})",
          "added_until": "${target_user:user} was added to the group “${group:group}” by ${author:user} until ${expiration_date:date}. (${reason})",
          "removed": "${target_user:user} was removed from the group “${group:group}” by ${author:user}. (${reason})"
        },
        "entry_automatic": {
          "added": "${target_user:user} was added to the group “${group:group}”. (${reason})",
          "added_until": "${target_user:user} was added to the group “${group:group}” until ${expiration_date:date}. (${reason})",
          "removed": "${target_user:user} was removed from the group “${group:group}”. (${reason})"
        }
      },
      "page_protection": {
        "title": "Page protection log",
        "entry": "${author:user} has set the protection level of ${full_page_title:link} to “${protection_level:group}” until ${expiration_date:date}. (${reason})",
        "entry_no_date": "${author:user} has set the protection level of ${full_page_title:link} to “${protection_level:group}” indefinitely. (${reason})",
        "entry_talk": "${author:user} has set the protection level of ${full_page_title:link} and its talk page to “${protection_level:group}” until ${expiration_date:date}. (${reason})",
        "entry_no_date_talk": "${author:user} has set the protection level of ${full_page_title:link} and its talk page to “${protection_level:group}” indefinitely. (${reason})"
      },
      "page_creation": {
        "title": "Page creation log",
        "entry": "${author:user} has created the page ${full_page_title:link}. (${reason})"
      },
